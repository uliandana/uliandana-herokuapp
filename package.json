{
  "name": "spotify-web",
  "version": "0.1.0",
  "description": "Source code of custom Spotify App web",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "build": "babel-node tools/build.js",
    "build:development": "npm run build -- --development",
    "build:production": "npm run build -- --production",
    "build:staging": "npm run build -- --staging",
    "dev": "concurrently -k -r \"npm:open:src\" \"npm:lint:watch\"",
    "heroku-postbuild": "npm run build -- --production",
    "lint": "esw src --color --ignore-pattern src/assets",
    "lint:watch": "npm run lint -- --watch",
    "open:src": "babel-node tools/srcServer.js",
    "start": "babel-node tools/distServer.js",
    "test": "jest --passWithNoTests",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.0",
    "axios": "^0.21.0",
    "fontsource-ubuntu": "^3.0.9",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.4",
    "redux-form": "^8.3.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-do-expressions": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-react-constant-elements": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "chalk": "^4.1.0",
    "compression-webpack-plugin": "^6.0.4",
    "concurrently": "^5.3.0",
    "connect-gzip-static": "^2.1.1",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^6.2.1",
    "coveralls": "^3.1.0",
    "css-loader": "^5.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.12.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-watch": "^7.0.0",
    "express": "^4.17.1",
